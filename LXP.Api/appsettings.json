    "Serilog": {
      "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
      "WriteTo": [
        {
          "Name": "Console"
        },
        {
          "Name": "File",
          "Args": {
            "Path": "logs/ConfigLog-.txt",
            "rollingInterval": "Day"
          }
        }
      ]
    
  },
  "AllowedHosts": "*"
}
